{
    "manifest_version": 3,
    "name": "LinkedIn ConnectEz",
    "version": "1.0.0",
    "description": "Automate your LinkedIn networking with AI-powered connections",
    "permissions": [
        "storage",
        "alarms",
        "tabs"
    ],
    "host_permissions": [
        "http://localhost:3000/*",
        "https://www.linkedin.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options.html",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}